{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Clone Instagram",
  "main": "index.js",
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "npx prisma generate",
    "prisma": "run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enjoy-JSB/prismagram.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Enjoy-JSB/prismagram/issues"
  },
  "homepage": "https://github.com/Enjoy-JSB/prismagram#readme",
  "devDependencies": {
    "nodemon": "^2.0.4",
    "regenerator-runtime": "^0.13.7"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@graphql-tools/load-files": "^6.2.3",
    "@graphql-tools/merge": "^6.2.3",
    "apollo-server": "^2.18.1",
    "aws-sdk": "^2.804.0",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "dotenv": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.12",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10",
    "yarn": "^1.22.10"
  }
}
